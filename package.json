{
  "name": "bioinformatics-js",
  "version": "1.3.1",
  "description": "A comprehensive React library for bioinformatics visualization tools including DNA sequence viewers, annotation management, and more.",
  "type": "module",
  "main": "dist/index.js",
  "module": "dist/index.esm.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.esm.js",
      "require": "./dist/index.js",
      "types": "./dist/index.d.ts"
    }
  },
  "files": [
    "dist",
    "README.md",
    "LICENSE"
  ],
  "scripts": {
    "build": "rollup -c && tsc --emitDeclarationOnly",
    "dev": "rollup -c --watch",
    "prepublishOnly": "npm run build"
  },
  "keywords": [
    "bioinformatics",
    "dna",
    "sequence",
    "visualization",
    "react",
    "d3",
    "genomics",
    "annotation",
    "biology"
  ],
  "author": "Your Name",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/yourusername/bioinformatics-js.git"
  },
  "bugs": {
    "url": "https://github.com/yourusername/bioinformatics-js/issues"
  },
  "homepage": "https://github.com/yourusername/bioinformatics-js#readme",
  "peerDependencies": {
    "react": ">=16.8.0",
    "react-dom": ">=16.8.0"
  },
  "dependencies": {
    "d3": "^7.8.5"
  },
  "devDependencies": {
    "@types/d3": "^7.4.0",
    "@types/react": "^18.2.0",
    "@types/react-dom": "^18.2.0",
    "typescript": "^5.1.0",
    "rollup": "^3.29.0",
    "@rollup/plugin-typescript": "^11.1.0",
    "@rollup/plugin-node-resolve": "^15.2.0",
    "rollup-plugin-peer-deps-external": "^2.2.4",
    "tslib": "^2.6.0"
  }
}